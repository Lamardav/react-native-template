{
  "name": "RNTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ts": "tsc -p .",
    "tsw": "npm run ts -- --watch",
    "pod-install-common": "cd ios && pod install --verbose",
    "pod-install-new-arch": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install --verbose",
    "start": "react-native start",
    "start-reset-cache": "react-native start --reset-cache",
    "android:run-dev": "node node_modules/react-native/local-cli/cli.js run-android --no-packager --variant debug",
    "android:run-test": "node node_modules/react-native/local-cli/cli.js run-android --no-packager --variant qaDebug",
    "android:run-stage": "node node_modules/react-native/local-cli/cli.js run-android --no-packager --variant stageDebug",
    "android:run-prod": "node node_modules/react-native/local-cli/cli.js run-android --no-packager --variant prodDebug",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "generate-image-resources": "img-res-gen --dir=resources/images --out=src/common/ImageResources.g.ts --ts",
    "postinstall": "patch-package && node tools/init.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@reduxjs/toolkit": "2.2.3",
    "dayjs": "1.11.10",
    "deprecated-react-native-prop-types": "5.0.0",
    "lodash": "4.17.21",
    "patch-package": "8.0.0",
    "react": "18.2.0",
    "react-hook-form": "7.51.2",
    "react-i18next": "14.1.0",
    "react-native": "0.72.12",
    "react-native-config": "1.4.6",
    "react-native-device-detection": "0.2.1",
    "react-native-fast-image": "8.6.3",
    "react-native-gesture-handler": "2.16.0",
    "react-native-image-resource-generator": "1.0.2",
    "react-native-localize": "3.1.0",
    "react-native-modalize": "2.1.1",
    "react-native-navigation": "7.39.1",
    "react-native-navigation-hooks": "6.3.0",
    "react-native-orientation-locker": "1.6.0",
    "react-native-reanimated": "3.8.1",
    "react-native-snap-carousel": "3.9.1",
    "react-native-svg": "13.14.0",
    "react-native-tab-view": "3.5.2",
    "react-redux": "9.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.12",
    "@tsconfig/react-native": "^3.0.0",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.0.24",
    "@types/react-native-snap-carousel": "^3.8.11",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.13",
    "babel-jest": "^29.2.1",
    "babel-plugin-root-import": "6.6.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.76.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=18"
  }
}
